{
  "name": "@brixtol/currency-symbol-placements",
  "version": "1.0.0",
  "description": "Mappings for Currency Symbol placements using currency.js patterns",
  "author": {
    "name": "Νίκος Σαβίδης ",
    "email": "n@brixtol.com",
    "url": "https://brixtoltextiles.com"
  },
  "license": "MIT",
  "main": "./package/index.js",
  "exports": {
    "require": "./package/index.js",
    "import": "./package/index.es.js"
  },
  "types": "./package/index.d.ts",
  "keywords": [
    "currency",
    "mappings",
    "country",
    "3166-1 alpha-2",
    "ISO 4217"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brixtol/currency-symbol-placements"
  },
  "bugs": {
    "url": "https://github.com/brixtol/currency-symbol-placements"
  },
  "engines": {
    "pnpm": ">=5"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c --environment prod",
    "test": "ava --watch --verbose"
  },
  "ava": {
    "require": [
      "esm"
    ],
    "files": [
      "test/*.js"
    ],
    "concurrency": 5
  },
  "devDependencies": {
    "@brixtol/rollup-utils": "workspace:^0.2.0",
    "@brixtol/tsconfig": "workspace:^1.0.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "currency.js": "^2.0.4",
    "esm": "^3.2.25",
    "rollup": "^2.56.2",
    "rollup-plugin-filesize": "^9.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.3.5"
  }
}
